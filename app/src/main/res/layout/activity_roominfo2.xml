<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ScrollView
        android:id="@+id/scrollView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <android.widget.Button
                android:id="@+id/package_btn"
                android:layout_width="wrap_content"
                android:layout_height="430dp"
                android:layout_marginTop="40dp"
                android:background="@drawable/roundrectangle"
                android:contentDescription="package_btn"
                android:drawableTop="@drawable/round_rectangle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="45dp"
                tools:layout_editor_absoluteY="395dp" />

            <TextView
                android:id="@+id/package_str"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="160dp"
                android:text="6인 온돌 룸"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn"
                app:layout_constraintTop_toTopOf="@id/package_btn" />

            <TextView
                android:id="@+id/room_info_str"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="2dp"
                android:text="(거실1, 작은 방1, 화장실1)"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@id/package_btn"
                app:layout_constraintTop_toBottomOf="@id/package_str" />

            <TextView
                android:id="@+id/personnel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="40dp"
                android:text="기준 40명/ 최대 40명"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@id/package_btn"
                app:layout_constraintTop_toBottomOf="@id/room_info_str" />

            <TextView
                android:id="@+id/price_person"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="40dp"
                android:text="1인"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn"
                app:layout_constraintTop_toBottomOf="@id/personnel" />

            <TextView
                android:id="@+id/price_person_int"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="25dp"
                android:text="25,000원"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@id/package_btn"
                app:layout_constraintTop_toBottomOf="@id/personnel" />

            <TextView
                android:id="@+id/all_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="40dp"
                android:text="합계 금액"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn"
                app:layout_constraintTop_toBottomOf="@id/price_person" />

            <TextView
                android:id="@+id/all_price_int"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:text="600,000원"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@id/price_person_int"
                app:layout_constraintTop_toBottomOf="@id/price_person" />

            <Button
                android:id="@+id/package_btn2"
                android:layout_width="wrap_content"
                android:layout_height="430dp"
                android:layout_marginTop="40dp"
                android:background="@drawable/roundrectangle"
                android:drawableTop="@drawable/round_rectangle2"
                app:layout_constraintStart_toStartOf="@+id/package_btn"
                app:layout_constraintTop_toBottomOf="@id/package_btn"
                tools:layout_editor_absoluteX="45dp"
                tools:layout_editor_absoluteY="395dp"
                android:contentDescription="package_btn2"/>

            <TextView
                android:id="@+id/package_str2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="160dp"
                android:text="3인 온돌 룸"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn2"
                app:layout_constraintTop_toTopOf="@id/package_btn2" />

            <TextView
                android:id="@+id/room_info_str2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="2dp"
                android:text="(거실1, 작은 방1, 화장실1)"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@id/package_btn2"
                app:layout_constraintTop_toBottomOf="@id/package_str2" />

            <TextView
                android:id="@+id/personnel2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="40dp"
                android:text="기준 40명/ 최대 40명"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@id/package_btn2"
                app:layout_constraintTop_toBottomOf="@id/room_info_str2" />

            <TextView
                android:id="@+id/price_person2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="40dp"
                android:text="1인"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn2"
                app:layout_constraintTop_toBottomOf="@id/personnel2" />

            <TextView
                android:id="@+id/price_person_int2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="25dp"
                android:text="25,000원"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@id/package_btn2"
                app:layout_constraintTop_toBottomOf="@id/personnel2" />

            <TextView
                android:id="@+id/all_price2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="40dp"
                android:text="합계 금액"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn2"
                app:layout_constraintTop_toBottomOf="@id/price_person2" />

            <TextView
                android:id="@+id/all_price_int2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:text="600,000원"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@id/price_person_int2"
                app:layout_constraintTop_toBottomOf="@id/price_person2" />

            <android.widget.Button
                android:id="@+id/package_btn3"
                android:layout_width="wrap_content"
                android:layout_height="430dp"
                android:layout_marginTop="40dp"
                android:background="@drawable/roundrectangle"
                android:drawableTop="@drawable/round_rectangle2"
                app:layout_constraintTop_toBottomOf="@id/package_btn2"
                tools:layout_editor_absoluteX="45dp"
                tools:layout_editor_absoluteY="395dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/package_str3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="160dp"
                android:text="소형 온돌 룸"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/package_btn3"
                app:layout_constraintTop_toTopOf="@id/package_btn3" />

            <TextView
                android:id="@+id/room_info_str3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="2dp"
                android:text="(거실1, 작은 방1, 화장실1)"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@id/package_btn3"
